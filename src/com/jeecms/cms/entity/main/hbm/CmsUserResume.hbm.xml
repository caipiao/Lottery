<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.jeecms.cms.entity.main">
	<class name="CmsUserResume" table="jc_user_resume">
		<meta attribute="sync-DAO">false</meta>
		<cache usage="read-write"/>
		<id name="id" type="java.lang.Integer" column="user_id">
			<generator class="foreign"><param name="property">user</param></generator>
		</id>
		<property
			name="resumeName"
			column="resume_name"
			type="string"
			not-null="true"
			length="255"
		/>
		<property
			name="targetWorknature"
			column="target_worknature"
			type="string"
			not-null="false"
			length="255"
		/>
		<property
			name="targetWorkplace"
			column="target_workplace"
			type="string"
			not-null="false"
			length="255"
		/>
		<property
			name="targetCategory"
			column="target_category"
			type="string"
			not-null="false"
			length="255"
		/>
		<property
			name="targetSalary"
			column="target_salary"
			type="string"
			not-null="false"
			length="255"
		/>
		<property
			name="eduSchool"
			column="edu_school"
			type="string"
			not-null="false"
			length="255"
		/>
		<property
			name="eduGraduation"
			column="edu_graduation"
			type="timestamp"
			not-null="false"
			length="19"
		/>
		<property
			name="eduBack"
			column="edu_back"
			type="string"
			not-null="false"
			length="255"
		/>
		<property
			name="eduDiscipline"
			column="edu_discipline"
			type="string"
			not-null="false"
			length="255"
		/>
		<property
			name="recentCompany"
			column="recent_company"
			type="string"
			not-null="false"
			length="500"
		/>
		<property
			name="companyIndustry"
			column="company_industry"
			type="string"
			not-null="false"
			length="255"
		/>
		<property
			name="companyScale"
			column="company_scale"
			type="string"
			not-null="false"
			length="255"
		/>
		<property
			name="jobName"
			column="job_name"
			type="string"
			not-null="false"
			length="255"
		/>
		<property
			name="jobCategory"
			column="job_category"
			type="string"
			not-null="false"
			length="255"
		/>
		<property
			name="jobStart"
			column="job_start"
			type="timestamp"
			not-null="false"
			length="19"
		/>
		<property
			name="subordinates"
			column="subordinates"
			type="string"
			not-null="false"
			length="255"
		/>
		<property
			name="jobDescription"
			column="job_description"
			type="string"
			not-null="false"
		/>
		<property
			name="selfEvaluation"
			column="self_evaluation"
			type="string"
			not-null="false"
		/>
		<one-to-one name="user" class="CmsUser" constrained="true"/>
	</class>
</hibernate-mapping>